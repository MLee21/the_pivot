<%= form_for(@item, url: admin_items_path, html: {mulipart: true} ) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_field :description %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :image, "Attach image" %><br>
  <%= f.file_field :image %>

  <%= f.submit 'Create dog' %>

<% end %>

<%= render partial: "layouts/master_header" %>
<%= form_for(@admin, url: admin_admin_path(@admin)) do |f| %>
  <%= f.label :full_name %>
  <%= f.text_field :full_name %>

  <%= f.label :display_name %>
  <%= f.text_field :display_name %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %>
  <%= f.password_field :password_confirmation %>

  <%= f.submit "Update Admin" %>
<% end %>
 <%= render partial: "layouts/admin_navigation" %>
<header class="row">
  <div class="col-md-5">
    <%= image_tag("veg.jpg") %>
  </div><!-- col-md-4-->
  <div class="col-md-7 vendor-name">
    <h1>Welcome to <%= current_user.vendor.name %></h1>
  </div><!-- col-md-8-->
</header><!-- row -->
<h1>Business Administrators:</h1> 
<div>
  <ul id="admins_list">
<% @admins.each do |admin| %>
  <li><%= link_to admin.full_name, admin_admin_path(admin) %></li>
  <li><%= link_to "Edit Admin", edit_admin_admin_path(admin) %></li>
  <li><%= admin.email %></li>
<% end %>
  </ul>
</div>

 <%= button_to "Create Admin", new_admin_admin_path, method: :get %>

